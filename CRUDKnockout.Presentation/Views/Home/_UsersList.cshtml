
@{
    ViewBag.Title = "_UsersList";
}
<table class="table table-striped table-bordered table-condensed">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Gender</th>
            <th>Action</th>
            <th>UserDetail</th>
        </tr>
    </thead>
    
    <tbody data-bind="foreach: users">
        <tr>
            <td data-bind="text: Name"></td>
            <td data-bind="text: Age"></td>
            <td data-bind="text: Gender"></td>
            <td>
                <input type="button" class="btn btn-danger btn-xs" value=" [x] Delete " data-bind="click: $parent.removeUser" />
                <input type="button" class="btn btn-primary btn-xs" value="Edit " data-bind="click: $parent.EditUser" />
            </td>
            <td>
                <input type="button" class="btn btn-default btn-xs" value="Detail" data-bind="click: $parent.Detail" />
            </td>
        </tr>
    </tbody>
</table>

<br />



